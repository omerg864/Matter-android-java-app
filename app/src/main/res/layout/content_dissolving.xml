<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_dissolving"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.og.matter.Dissolving"
    tools:showIn="@layout/app_bar_dissolving">

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText2"
        android:hint="הכנס חומר יוני"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="41dp" />

    <TextView
        android:text="(s)"
        android:gravity="bottom|left"
        android:textSize="14sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/editText2"
        android:layout_toRightOf="@+id/editText2"
        android:layout_toEndOf="@+id/editText2"
        android:id="@+id/textView31"
        android:layout_alignBottom="@+id/editText2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="➙"
        android:textSize="20sp"
        android:gravity="center"
        android:id="@+id/textView23"
        android:layout_alignTop="@+id/textView31"
        android:layout_toRightOf="@+id/textView31"
        android:layout_toEndOf="@+id/textView31"
        android:layout_alignBottom="@+id/textView31" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="20sp"
        android:gravity="center"
        android:id="@+id/textView24"
        android:layout_toRightOf="@+id/textView23"
        android:layout_toEndOf="@+id/textView23"
        android:layout_alignBottom="@+id/textView23"
        android:layout_alignTop="@+id/textView23" />

    <TextView
        android:text=""
        android:gravity="top|left"
        android:textSize="13sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView27"
        android:layout_alignTop="@+id/textView24"
        android:layout_toRightOf="@+id/textView24"
        android:layout_toEndOf="@+id/textView24"
        android:layout_alignBottom="@+id/textView24" />

    <TextView
        android:text=""
        android:gravity="bottom|left"
        android:textSize="14sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView28"
        android:layout_toRightOf="@+id/textView27"
        android:layout_toEndOf="@+id/textView27"
        android:layout_alignBottom="@+id/textView27"
        android:layout_alignTop="@+id/textView27" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="20sp"
        android:gravity="center"
        android:id="@+id/textView25"
        android:layout_alignBottom="@+id/textView28"
        android:layout_toEndOf="@+id/textView28"
        android:layout_alignTop="@+id/textView27"
        android:layout_toRightOf="@+id/textView28" />

    <TextView
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="20sp"
        android:gravity="center"
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_alignTop="@+id/textView25"
        android:layout_toRightOf="@+id/textView25"
        android:layout_toEndOf="@+id/textView25"
        android:layout_alignBottom="@+id/textView25" />

    <TextView
        android:text=""
        android:gravity="top|left"
        android:textSize="13sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView29"
        android:layout_alignTop="@+id/textView26"
        android:layout_toRightOf="@+id/textView26"
        android:layout_toEndOf="@+id/textView26"
        android:layout_alignBottom="@+id/textView26" />

    <TextView
        android:text=""
        android:gravity="bottom|left"
        android:textSize="14sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView30"
        android:layout_alignTop="@+id/textView29"
        android:layout_toRightOf="@+id/textView29"
        android:layout_toEndOf="@+id/textView29"
        android:layout_alignBottom="@+id/textView29" />

    <Space
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="20dp"
        android:id="@+id/space5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="בצע תהליך"
        android:onClick="dissolving"
        android:id="@+id/button13"
        android:layout_alignBottom="@+id/space5"
        android:layout_toLeftOf="@+id/space5"
        android:layout_toStartOf="@+id/space5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="נקה"
        android:onClick="cleandissolving"
        android:id="@+id/button14"
        android:layout_alignBottom="@+id/space5"
        android:layout_toRightOf="@+id/space5"
        android:layout_toEndOf="@+id/space5" />

</RelativeLayout>
